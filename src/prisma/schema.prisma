generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model antispam {
  guild        String   @id
  setting      Boolean?
  messagecount Int?
  interval     Int?
}

model warnsys {
  guild     String   @id
  mutelimit Int?
  kicklimit Int?
  banlimit  Int?
  onspam    Boolean?
  duration  Int?
}

model warns {
  id    String @id
  user  String
  guild String
  count Int?
}

model ignoretable {
  id    String  @id
  user  String?
  guild String?
}

model sticky {
  id      String @id
  guild   String
  channel String
  content String
}
